{
  "manifest_version": 2,
  "name": "Distract Blocker",
  "version": "1.0",
  "description": "Blocks distracting websites",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  }
}

